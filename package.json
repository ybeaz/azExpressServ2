  "scripts": {
    "copyDistFiles": "node ./tools/copyDistributionFiles.ts",
    "givePermission": "node ./tools/givePermission.ts",
    "prebuild": "rimraf dist",
    "build": "yarn copyDistFiles && yarn tsc --build && yarn givePermission",
    "serve": "NODE_ENV=development nodemon --trace-warnings --trace-deprecation --exec ./dist/index.js",
    "build+serve": "yarn build && yarn serve",
    "start": "NODE_ENV=production nodemon ./index.js",
    "dev:start": "open-cli http://localhost:8082/user/John/Smith & concurrently --kill-others --raw --success=first 'nodemon --config nodemon.config.json --ext js,ts,tsc,.json'",
    "dev": "concurrently --kill-others --raw --success=first 'nodemon --config nodemon.config.json --ext js,ts,tsc,.json'",
    "startAndGraphQL": "opn http://localhost:8082/graphql & opn http://localhost:8082/user/John/Smith & node index.js",
    "dev2": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
