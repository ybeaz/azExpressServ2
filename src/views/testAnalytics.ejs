    onclick="button00()">
    Button 00
  </button>
  <button class="button az_{'type':'click','name':'button01','level':2}" type="button"">Button 01</button>
    
    <input class=" checkbox az_{'type':'change','name':'checkbox00'}" type="checkbox" onchange="checkBox00()" />
  <input class="checkbox az_{'type':'change','name':'checkbox01'}" type="checkbox" />

  <input class="inputText az_{'type':'blur','name':'input01'}" type="text" oninput="inputText00()" />
  <input class="inputText az_{'type':'mouseleave','name':'input02'}" type="text" />

